<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@drawable/popup_border">    <ImageButton        android:layout_width="@dimen/cancelButton_width"        android:layout_height="@dimen/cancelButton_width"        android:id="@+id/cancel"        android:background="@drawable/cancel"        android:layout_alignParentTop="true"        android:layout_alignParentEnd="true"        android:layout_margin="@dimen/cancelButton_margin"        android:onClick="cancelGame" />    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:textSize="@dimen/title_textSize"        android:text="Level"        android:theme="@style/AppTheme.BoldAndItalic"        android:textColor="#FFFFFF"        android:id="@+id/popUpLevelTitle"        android:layout_marginTop="@dimen/title_marginTop"        android:layout_centerHorizontal="true" />    <GridLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:id="@+id/currStarsGrid"        android:rowCount="1"        android:columnCount="3"        android:layout_below="@id/popUpLevelTitle"        android:layout_marginTop="@dimen/level_info_stars_marginTop"        android:layout_centerHorizontal="true"        android:useDefaultMargins="true">        <ImageView            android:layout_width="@dimen/level_info_star_width"            android:layout_height="@dimen/level_info_star_width"            android:id="@+id/star1"            android:tag="1"            android:layout_row="0"            android:layout_column="0"            android:background="@drawable/star" />        <ImageView            android:layout_width="@dimen/level_info_star_width"            android:layout_height="@dimen/level_info_star_width"            android:tag="2"            android:id="@+id/star2"            android:layout_row="0"            android:layout_column="1"            android:background="@drawable/star" />        <ImageView            android:layout_width="@dimen/level_info_star_width"            android:layout_height="@dimen/level_info_star_width"            android:id="@+id/star3"            android:tag="3"            android:layout_row="0"            android:layout_column="2"            android:background="@drawable/star" />    </GridLayout>    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:textSize="@dimen/level_info_desc_textSize"        android:text="Add to 8 for most points in 10 moves. "        android:textColor="#FFFFFF"        android:id="@+id/gameType"        android:layout_below="@+id/currStarsGrid"        android:layout_margin="@dimen/level_info_desc_margin"        android:layout_centerHorizontal="true"        android:gravity="center_horizontal" />    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:textSize="@dimen/level_info_desc_textSize"        android:text="Extra tasks"        android:textColor="#FFFFFF"        android:theme="@style/AppTheme.BoldAndItalic"        android:layout_below="@+id/gameType"        android:id="@+id/extraTasks"        android:gravity="center_horizontal"        android:layout_centerHorizontal="true" />    <LinearLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/extraTasks"        android:id="@+id/otherTasks"        android:orientation="vertical"        android:layout_marginLeft="@dimen/level_info_desc_margin">        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:textSize="@dimen/level_info_task_desc_textSize"            android:text="- Break all the glass"            android:textColor="#FFFFFF" />    </LinearLayout>    <GridLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:id="@+id/starGrid"        android:rowCount="3"        android:columnCount="4"        android:orientation="horizontal"        android:layout_below="@+id/otherTasks"        android:layout_centerHorizontal="true"        android:useDefaultMargins="true"        android:layout_marginTop="@dimen/level_info_star_points_marginTop">        <ImageView            android:layout_width="@dimen/level_info_star_points_width"            android:layout_height="@dimen/level_info_star_points_width"            android:layout_row="0"            android:layout_column="0"            android:background="@drawable/star" />        <ImageView            android:layout_width="@dimen/level_info_star_points_width"            android:layout_height="@dimen/level_info_star_points_width"            android:layout_row="0"            android:layout_column="1"            android:background="@drawable/star_empty_blk" />        <ImageView            android:layout_width="@dimen/level_info_star_points_width"            android:layout_height="@dimen/level_info_star_points_width"            android:layout_row="0"            android:layout_column="2"            android:background="@drawable/star_empty_blk" />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:id="@+id/star1Points"            android:textSize="@dimen/level_info_star_points_textSize"            android:text="200 points"            android:paddingLeft="@dimen/level_info_star_points_padding_left"            android:textColor="#FFFFFF"            android:layout_row="0"            android:layout_column="3"            android:gravity="center_vertical" />        <ImageView            android:layout_width="@dimen/level_info_star_points_width"            android:layout_height="@dimen/level_info_star_points_width"            android:layout_row="1"            android:layout_column="0"            android:background="@drawable/star" />        <ImageView            android:layout_width="@dimen/level_info_star_points_width"            android:layout_height="@dimen/level_info_star_points_width"            android:layout_row="1"            android:layout_column="1"            android:background="@drawable/star" />        <ImageView            android:layout_width="@dimen/level_info_star_points_width"            android:layout_height="@dimen/level_info_star_points_width"            android:layout_row="1"            android:layout_column="2"            android:background="@drawable/star_empty_blk" />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:id="@+id/star2Points"            android:textSize="@dimen/level_info_star_points_textSize"            android:text="400 points"            android:paddingLeft="@dimen/level_info_star_points_padding_left"            android:textColor="#FFFFFF"            android:layout_row="1"            android:layout_column="3"            android:gravity="center_vertical" />        <ImageView            android:layout_width="@dimen/level_info_star_points_width"            android:layout_height="@dimen/level_info_star_points_width"            android:id="@+id/star3_1"            android:layout_row="2"            android:layout_column="0"            android:background="@drawable/star" />        <ImageView            android:layout_width="@dimen/level_info_star_points_width"            android:layout_height="@dimen/level_info_star_points_width"            android:layout_row="2"            android:layout_column="1"            android:background="@drawable/star" />        <ImageView            android:layout_width="@dimen/level_info_star_points_width"            android:layout_height="@dimen/level_info_star_points_width"            android:layout_row="2"            android:layout_column="2"            android:background="@drawable/star" />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:id="@+id/star3Points"            android:textSize="@dimen/level_info_star_points_textSize"            android:text="600 points"            android:paddingLeft="@dimen/level_info_star_points_padding_left"            android:textColor="#FFFFFF"            android:layout_row="2"            android:layout_column="3"            android:gravity="center_vertical" />    </GridLayout>    <Button        android:layout_width="@dimen/single_button_width"        android:layout_height="@dimen/single_button_height"        android:text="Start"        android:textColor="#ffffff"        android:textSize="@dimen/single_button_textSize"        android:id="@+id/startButton"        android:background="@drawable/my_button"        android:layout_centerHorizontal="true"        android:layout_below="@+id/starGrid"        android:layout_margin="@dimen/level_info_button_marginTop" /></RelativeLayout>