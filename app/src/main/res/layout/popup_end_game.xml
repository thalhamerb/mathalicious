<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@drawable/popup_border">    <ImageButton        android:layout_width="@dimen/cancelButton_width"        android:layout_height="@dimen/cancelButton_width"        android:id="@+id/levelScreenButton"        android:layout_gravity="right|top"        android:background="@mipmap/cancel"        android:layout_alignParentTop="true"        android:layout_alignParentEnd="true"        android:layout_margin="@dimen/cancelButton_margin" />    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:textSize="@dimen/title_textSize"        android:text="Game Ended"        android:theme="@style/AppTheme.BoldAndItalic"        android:textColor="#FFFFFF"        android:id="@+id/mainMessage"        android:layout_marginTop="@dimen/title_marginTop"        android:layout_centerHorizontal="true" />    <GridLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:id="@+id/starGrid"        android:rowCount="1"        android:columnCount="3"        android:layout_below="@id/mainMessage"        android:layout_marginTop="@dimen/end_game_stars_marginTop"        android:layout_centerHorizontal="true"        android:useDefaultMargins="true">        <ImageView            android:layout_width="@dimen/end_game_star_width"            android:layout_height="@dimen/end_game_star_width"            android:id="@+id/star1"            android:tag="1"            android:layout_row="0"            android:layout_column="0"            android:background="@mipmap/star" />        <ImageView            android:layout_width="@dimen/end_game_star_width"            android:layout_height="@dimen/end_game_star_width"            android:tag="2"            android:id="@+id/star2"            android:layout_row="0"            android:layout_column="1"            android:background="@mipmap/star" />        <ImageView            android:layout_width="@dimen/end_game_star_width"            android:layout_height="@dimen/end_game_star_width"            android:id="@+id/star3"            android:tag="3"            android:layout_row="0"            android:layout_column="2"            android:background="@mipmap/star" />    </GridLayout>    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:textSize="@dimen/end_game_score_textSize"        android:text="Score: 5000 points"        android:textColor="#FFFFFF"        android:id="@+id/score"        android:layout_below="@id/starGrid"        android:layout_marginTop="@dimen/end_game_score_margin"        android:layout_centerHorizontal="true" />    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:textSize="@dimen/level_info_desc_textSize"        android:text="Extra tasks"        android:textColor="#FFFFFF"        android:layout_below="@+id/score"        android:id="@+id/extraTasks"        android:layout_marginTop="20dp"        android:gravity="center_horizontal"        android:layout_centerHorizontal="true" />    <LinearLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/extraTasks"        android:id="@+id/otherTasks"        android:orientation="vertical"        android:layout_marginLeft="@dimen/level_info_desc_margin">        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:textSize="@dimen/level_info_desc_textSize"            android:text="- Pop all bubbles."            android:textColor="#FFFFFF" />    </LinearLayout>    <RelativeLayout        android:layout_width="wrap_content"        android:layout_height="@dimen/end_game_thought_height"        android:layout_below="@id/otherTasks"        android:id="@+id/thought_bubble_frame"        android:layout_marginLeft="@dimen/end_game_thought_margin_horiz"        android:layout_marginRight="@dimen/end_game_thought_margin_horiz"        android:layout_marginTop="@dimen/end_game_thought_margin_vert"        android:layout_marginBottom="@dimen/end_game_thought_margin_vert">        <ImageView            android:layout_width="fill_parent"            android:layout_height="fill_parent"            android:background="@mipmap/thought_bubble"            android:id="@+id/thought_bubble"            android:layout_row="0"            android:layout_column="0" />        <TextView            android:layout_width="fill_parent"            android:layout_height="fill_parent"            android:textSize="@dimen/end_game_thought_textSize"            android:text="Win one more game and I'll tell you the secret to my hair...and maybe time travel."            android:id="@+id/thought_bubble_text"            android:layout_margin="@dimen/end_game_thought_inner_margin"            android:layout_row="0"            android:layout_column="1" />    </RelativeLayout>    <RelativeLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@id/thought_bubble_frame"        android:layout_centerHorizontal="true">        <ImageView            android:layout_width="@dimen/end_game_char_width"            android:layout_height="@dimen/end_game_char_height"            android:src="@mipmap/einstein_full_body"            android:id="@+id/character"            android:layout_margin="@dimen/end_game_button_grid_margin" />        <GridLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:id="@+id/buttonGrid"            android:rowCount="2"            android:columnCount="1"            android:layout_toRightOf="@id/character"            android:layout_centerVertical="true"            android:layout_marginLeft="@dimen/end_game_button_grid_margin">            <Button                android:layout_width="@dimen/end_game_button_width"                android:layout_height="@dimen/end_game_button_height"                android:text="Retry"                android:textColor="#ffffff"                android:textSize="@dimen/end_game_button_textSize"                android:id="@+id/retryButton"                android:background="@drawable/popup_button"                android:layout_margin="@dimen/end_game_button_margin"                android:layout_row="0"                android:layout_column="0" />            <Button                android:layout_width="@dimen/end_game_button_width"                android:layout_height="@dimen/end_game_button_height"                android:text="Next"                android:textColor="#ffffff"                android:textSize="@dimen/end_game_button_textSize"                android:id="@+id/nextButton"                android:background="@drawable/popup_button"                android:layout_margin="@dimen/end_game_button_margin"                android:layout_row="1"                android:layout_column="0" />        </GridLayout>    </RelativeLayout></RelativeLayout>